{
    "plugins": ["node_modules/jsdoc-vuejs"],
    "recurseDepth": 30,
    "source": {
        "include": ["src", "mixins"],
        "includePattern": ".+\\.(vue|js(doc|x)?)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true,
        "verbose": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
